  author = {Wei, Chen and Qian, Pierre and Tedrow, Usha and Mak, Raymond and Zei, Paul C},
  date = {2019-12},
  journaltitle = {Arrhythmia \& Electrophysiology Review},
  shortjournal = {Arrhythm Electrophysiol Rev},
