@article{clunieDICOMFormatProtocol2021,
  title = {{{DICOM Format}} and {{Protocol Standardization}}—{{A Core Requirement}} for {{Digital Pathology Success}}},
  author = {Clunie, David A.},
  date = {2021-06-01},
  journaltitle = {Toxicologic Pathology},
  shortjournal = {Toxicol Pathol},
  volume = {49},
